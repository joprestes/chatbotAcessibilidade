# Google Gemini (obrigatório)
GOOGLE_API_KEY=your_google_api_key

# OpenRouter (opcional, necessário para fallback)
OPENROUTER_API_KEY=your_openrouter_api_key
FALLBACK_ENABLED=false
OPENROUTER_MODELS=meta-llama/llama-3.3-70b-instruct:free,google/gemini-flash-1.5:free,mistralai/mistral-7b-instruct:free
OPENROUTER_TIMEOUT_SECONDS=60

# CORS (opcional)
CORS_ORIGINS=*

# Rate Limiting (opcional)
RATE_LIMIT_ENABLED=true
RATE_LIMIT_PER_MINUTE=10

# Validação de entrada (opcional)
MAX_QUESTION_LENGTH=2000
MIN_QUESTION_LENGTH=3

# Timeout (opcional)
API_TIMEOUT_SECONDS=60

# Cache (opcional)
CACHE_ENABLED=true
CACHE_TTL_SECONDS=3600
CACHE_MAX_SIZE=100

# Logging (opcional)
LOG_LEVEL=INFO
LOG_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(message)s
