# Google Gemini (obrigatório)
GOOGLE_API_KEY=your_google_api_key

# Hugging Face (opcional, necessário para fallback)
HUGGINGFACE_API_KEY=your_huggingface_api_key
FALLBACK_ENABLED=false
HUGGINGFACE_MODELS=meta-llama/Llama-3.3-70B-Instruct,google/gemma-2-9b-it,mistralai/Mistral-7B-Instruct-v0.3
HUGGINGFACE_TIMEOUT_SECONDS=60

# CORS (opcional)
CORS_ORIGINS=*

# Rate Limiting (opcional)
RATE_LIMIT_ENABLED=true
RATE_LIMIT_PER_MINUTE=10

# Validação de entrada (opcional)
MAX_QUESTION_LENGTH=2000
MIN_QUESTION_LENGTH=3

# Timeout (opcional)
API_TIMEOUT_SECONDS=60

# Cache (opcional)
CACHE_ENABLED=true
CACHE_TTL_SECONDS=3600
CACHE_MAX_SIZE=100

# Logging (opcional)
LOG_LEVEL=INFO
LOG_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(message)s
